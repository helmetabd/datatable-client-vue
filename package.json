{
  "name": "datatable-client",
  "version": "1.0.0",
  "description": "A Datatable Component with some custom use bootstrap",
  "private": false,
  "type": "module",
  "main": "./dist/datatable-client.umd.cjs",
  "module": "./dist/datatable-client.js",
  "types": "./dist/datatable.d.ts",
  "author": "Hilmi Abdurrahim",
  "license": "MIT",
  "files": [
    "dist",
    "src/assets",
    "src/components"
  ],
  "exports": {
    ".": {
      "import": "./dist/datatable-client.js",
      "require": "./dist/datatable-client.umd.cjs"
    },
    "./dist/style.css": {
      "import": "./dist/style.css",
      "require": "./dist/style.css"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/"
  },
  "dependencies": {
    "@popperjs/core": "^2.11.8",
    "bootstrap": "^5.3.3",
    "clipboard": "^2.0.11",
    "jszip": "^3.10.1",
    "moment": "^2.30.1",
    "sweetalert2": "^11.12.4",
    "vue": "^3.4.38",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.8.0",
    "@tsconfig/node20": "^20.1.4",
    "@types/node": "^20.14.5",
    "@vitejs/plugin-vue": "^5.0.5",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^13.0.0",
    "@vue/tsconfig": "^0.5.1",
    "eslint": "^8.57.0",
    "eslint-plugin-vue": "^9.23.0",
    "rollup-plugin-typescript2": "^0.36.0",
    "npm-run-all2": "^6.2.0",
    "prettier": "^3.2.5",
    "sass": "^1.77.8",
    "typescript": "~5.5.4",
    "vite": "^5.3.1",
    "vite-plugin-dts": "^4.0.3",
    "vite-plugin-static-copy": "^1.0.6",
    "vue-tsc": "^2.0.21"
  },
  "peerDependencies": {
    "bootstrap": "^5.0.0",
    "vue": "^3.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/helmetabd/datatable-client-vue.git"
  }
}
